{
  "name": "@adonisjs/session",
  "version": "1.0.28",
  "description": "Session provider for AdonisJs",
  "main": "build/providers/SessionProvider.js",
  "files": [
    "build/src",
    "build/adonis-typings",
    "build/providers"
  ],
  "typings": "./build/adonis-typings/index.d.ts",
  "dependencies": {
    "@poppinss/utils": "^1.0.3",
    "@types/bson": "^4.0.0",
    "@types/lodash": "^4.14.135",
    "bson": "^4.0.2",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.13",
    "ms": "^2.1.2",
    "type-of-is": "^3.5.1",
    "uuid": "^3.3.2",
    "snyk": "^1.200.1"
  },
  "peerDependencies": {
    "adonisjs/core": "2.x.x"
  },
  "devDependencies": {
    "@adonisjs/core": "^2.0.4",
    "@adonisjs/fold": "^6.1.0",
    "@adonisjs/mrm-preset": "^2.0.3",
    "@poppinss/cookie": "^1.0.1",
    "@poppinss/dev-utils": "^1.0.0",
    "@poppinss/http-server": "^1.1.6",
    "@poppinss/logger": "^1.1.0",
    "@types/node": "^12.6.8",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "husky": "^3.0.1",
    "japa": "^2.0.10",
    "mrm": "^1.2.2",
    "pkg-ok": "^2.3.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "prepublishOnly": "npm run build",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "lint": "tslint --project tsconfig.json",
    "commit": "git-cz",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-session.git"
  },
  "keywords": [
    "session",
    "adonisjs"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-session/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-session#readme",
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "next",
    "access": "public"
  },
  "snyk": true
}
